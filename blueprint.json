{
  "schema": {
    "type": "app_blueprint",
    "version": {
      "major": 0,
      "minor": 1
    },
    "origin": "Engines Developer Studio 0.2"
  },
  "metadata": {
    "blueprint": {
      "version": {
        "major": 0,
        "minor": 0,
        "level": "alpha",
        "patch": 0
      }
    },
    "software": {
      "display": {
        "title": "Kill Bill"
      }
    },
    "timestamp": "2018-12-19T11:02:11+11:00"
  },
  "software": {
    "base": {
      "name": "killbill",
      "framework": "docker",
      "parent_image": "killbill/killbill:0.20.0",
      "run_as_user": "0",
      "deployment_type": "web",
      "http_protocol": "https_and_http",
      "framework_port_override": 8080,
      "memory": {
        "required": 256,
        "recommended": 256
      }
    },
    "ports": [
      {
        "port": "8000",
        "external": "8000",
        "protocol": "tcp"
      },
      {
        "port": "12345",
        "external": "12345",
        "protocol": "tcp"
      }
    ],
    "scripts": {
      "start": {
        "content": "#!/bin/sh\r\nsleep 600"
      }
    },
    "service_configurations": [
      {
        "publisher_namespace": "EnginesSystem",
        "type_path": "database/sql/mysql",
        "variables": {
          "database_name": "_Engines_Builder(engine_name)",
          "database_host": "_Engines_System(mysql_host)",
          "db_username": "_Engines_Builder(engine_name)",
          "db_password": "_Engines_System(random(10))",
          "collation": "utf8_general_ci",
          "full_access": "false"
        }
      }
    ],
    "environment_variables": [
      {
        "name": "KILLBILL_DAO_URL",
        "value": "jdbc:mysql://mysql:3306/_Engines_Environment(dbname)",
        "immutable": true,
        "input": {
          "type": "boolean"
        }
      },
      {
        "name": "KILLBILL_DAO_USER",
        "value": "_Engines_Environment(dbuser)",
        "immutable": true,
        "input": {
          "type": "hidden"
        }
      },
      {
        "name": "KILLBILL_DAO_PASSWORD",
        "value": "_Engines_Environment(passwd)",
        "immutable": true,
        "input": {
          "type": "boolean"
        }
      }
    ]
  }
}
